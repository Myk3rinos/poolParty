<%= render "shared/navbar" %>
<h1>Book your favourite pool</h1>

<% @bookings.each do |booking|%>
  <img src="<%= booking.pool.url_photo %>" alt="Image de <%= booking.pool.name %>">
  <p><strong><%= booking.pool.name %></strong></p>
  <%# <br> %>
  <%= button_to "cancel booking", booking_path(booking), method: :delete %>
<% end %>

<%= render "shared/footer" %>
